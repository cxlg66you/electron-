  "description": "The Cyber Swiss Army Knife",
  "main": "main.js",
  "author": {
    "name": "CyberChef",
    "email": "n1474335@gmail.com",
    "url": "https://github.com/gchq/CyberChef"
  },
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/gchq/CyberChef",
  "keywords": [
    "Electron",
    "CyberChef",
    "CyberSwissArmyKnife"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5"
  },
  "build": {
    "appId": "com. github.CyberChef",
    "mac": {
      "target": "dmg",
      "icon": "build/icons/icon.png"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            
            "x64"
          ]
        }
      ],
      "icon": "build/icons/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true,
      "shortcutName": "cyberchef"
    }
  },
  "dependencies": {
    "yarn": "^1.22.17"
  }
}
